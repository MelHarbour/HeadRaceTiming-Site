@model HeadRaceTimingSite.Models.Competition
@{
    ViewData["Title"] = Model.Name;
}

<paper-tabs selected="1">
    <paper-tab>Results</paper-tab>
    <paper-tab>Leaderboards</paper-tab>
</paper-tabs>

@foreach (var timingPoint in Model.TimingPoints) {
    <paper-card heading="@timingPoint.Name">
        <timingpoint-results-list competition-id="@Model.CompetitionId" timing-point-id="@timingPoint.TimingPointId"></timingpoint-results-list>
    </paper-card>
}